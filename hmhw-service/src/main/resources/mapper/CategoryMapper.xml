<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.weibo.project.hmhw.mapper.CategoryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.weibo.project.hmhw.model.Category">
        <id column="CATEGORY_ID" property="categoryId" />
        <result column="MODEL_CATEGORY" property="modelCategory" />
        <result column="category_type" property="categoryType" />
        <result column="category_code" property="categoryCode" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        CATEGORY_ID AS categoryId, MODEL_CATEGORY AS modelCategory, category_type AS categoryType, category_code AS categoryCode
    </sql>

</mapper>
